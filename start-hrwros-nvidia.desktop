[Desktop Entry]
Encoding=UTF-8
Name=HRWROS (Nvidia)
Comment=Startup the environment for HRWROS for computers with NVIDIA graphics cards.
Exec=$TERM -e '/bin/bash -c "/bin/mkdir -p /tmp/hrwros_run/dconf && XDG_RUNTIME_DIR=/tmp/hrwros_run /usr/bin/singularity shell -p --nv $HOME/software/ros-singularity/hrwros-09.simg"' --class=hrwros_mooc_class_nv
Icon=hrwros-mooc-icon-nv.png
Terminal=false
Categories=ROS;MOOC;HRWROS;
Type=Application
StartupWMClass=hrwros_mooc_class_nv
Actions=new-ccs
[Desktop Action new-ccs]
Name=New CCS (Nvidia)
Exec=$TERM -e '/bin/bash -c "/bin/mkdir -p /tmp/hrwros_run/dconf && XDG_RUNTIME_DIR=/tmp/hrwros_run /usr/bin/singularity shell -p --nv $HOME/software/ros-singularity/hrwros-09.simg"' --class=hrwros_mooc_class_nv
